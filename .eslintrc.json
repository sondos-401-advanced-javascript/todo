{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "fail": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "no-undef": 2,
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-unused-vars": "warn",
    "no-console": 1,
    "no-unexpected-multiline": "warn"
  },
  "parser": "babel-eslint"
}